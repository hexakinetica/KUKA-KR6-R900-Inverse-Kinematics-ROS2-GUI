<ros2_control name="KukaR900System" type="system">
  <hardware>
    <plugin>fake_components/GenericSystem</plugin>
    <param name="example_param_write_for_sec">0.1</param>
    <param name="disable_commands_on_start">false</param>
    <param name="disable_state_updates">false</param>
    <param name="fake_sensor_commands">true</param>
  </hardware>
  <joint name="joint_a1">
